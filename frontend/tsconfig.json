{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "composite": true,
        "jsx": "react-jsx",
        "baseUrl": ".",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "types": [
            "vite/client",
            "vitest"
        ],
        "paths": {
            "@/*": [
                "src/*"
            ]
        }
    },
    "include": [
        "src",
        "src/**/*.d.ts"
    ]
}